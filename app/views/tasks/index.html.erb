<div class="index">
    <h1>List</h1>
  <ul>
    <% @index.each do |task| %>
   <% if task.completed  %>
    <p> [X] <%= link_to task.title, task_path(task) %> </p>
    <% else %>
    <p>[] <%= link_to task.title, task_path(task) %></p>
    <% end %>
    <% end %>
  </ul>
</div>
<%= link_to "new", new_task_path %>
